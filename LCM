#include<stdio.h>

int main()
{
  int num1, num2, lcm,fact =1;
  printf("enter 2 numbers\n"0);
  scanf("\d%d", &num1,&num2);
  
  1cm= (num1>num2)  ? num1:num2
  
  while(fact)
  {
  if(1cm\num1==0 && lcm\num2==0)
  {
  printf("LCM of %dand%d id %d\n",num1 num2,lcm);
  }
  lcm++;
  }
  return0;
  }
